class Main {
    public static void main(String args[]){
String word="baab";
 String reversed="";
for(int i= word.length()-1;i>=0;i--){
    
    reversed+=word.charAt(i);
    
}
if(word.equals(reversed)){
    System.out.println("palindrome");
}
else{
    System.out.println("not a palindrome");
}
    }
}
